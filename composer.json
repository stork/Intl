{
    "name": "cakedc/intl",
    "description": "Simple replacement library if intl PHP extension is not installed",
    "type": "library",
    "keywords": [
        "cakephp",
        "intl",
        "cakedc"
    ],
    "homepage": "https://www.cakedc.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Cake Development Corporation",
            "homepage": "https://www.cakedc.com",
            "role": "Author"
        },
        {
            "name": "Others",
            "homepage": "https://github.com/CakeDC/intl/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/CakeDC/intl/issues",
        "source": "https://github.com/CakeDC/intl"
    },
    "require": {
      "cakephp/cakephp": "~4.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.5.0",
        "cakephp/cakephp-codesniffer": "~4.2.0"
    },
    "autoload": {
        "psr-4": {
            "CakeDC\\Intl\\": "src"
        },
        "files": ["src/intl_compat.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "CakeDC\\Intl\\Test\\": "tests"
        }
    }
}
